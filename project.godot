[application]

config/name="Lantern"
config/description="On her way to a robotics competition, Ember is confronted by a threatening stranger and flees into a strange forest full of bouncy mushrooms and magical warps."
run/main_scene="res://Level1.tscn"
boot_splash/image="res://assets/backgrounds/level_1.png"
boot_splash/bg_color=Color( 0.113725, 0.0823529, 0.341176, 1 )
config/icon="res://icon.png"

[autoload]

PluginSettings="*res://addons/github-integration/scripts/PluginSettings.gd"
IconLoaderGithub="*res://addons/github-integration/scripts/IconLoaderGithub.gd"
RestHandler="*res://addons/github-integration/scenes/RestHandler.tscn"
UserData="*res://addons/github-integration/scripts/user_data.gd"

[display]

window/size/width=160
window/size/height=160
window/size/test_width=640
window/size/test_height=640
window/stretch/mode="2d"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PoolStringArray( "github-integration" )

